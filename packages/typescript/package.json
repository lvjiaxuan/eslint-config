    "build": "nr j2t && tsup flat.mjs --format=esm,cjs --clean --splitting --target=node16 --treeshake",
    "prepublishOnly": "nr build"
  },
  "peerDependencies": {
    "eslint": ">=8",
    "typescript": ">=4"
  },
  "dependencies": {
    "@lvjiaxuan/eslint-config-js": "workspace:*",
