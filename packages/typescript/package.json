  "exports": {
    ".": "./index.cjs",
    "./flat": {
      "require": "./dist/flat.cjs",
      "import": "./dist/flat.mjs"
    }
  },
  "main": "./index.cjs",
  "scripts": {
    "dev": "nr build --watch",
    "j2t": "node js2ts",
