{
  "name": "@lvjiaxuan/eslint-config",
  "version": "0.10.0",
  "private": false,
  "author": "lvjiaxuan <471501748@qq.com> (https://github.com/lvjiaxuan)",
  "license": "MIT",
  "homepage": "https://github.com/lvjiaxuan/@lvjiaxuan/eslint-config-monorepo#README.md",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./index.cjs",
    "./flat": {
      "require": "./flat.cjs",
      "import": "./flat.mjs"
    }
  },
  "main": "index.cjs",
  "bin": "./bin/add.mjs",
  "files": [
    "dist",
    "./flat.cjs",
    "./flat.mjs",
    "!./add.ts"
  ],
  "scripts": {
    "build": "tsup add.ts --format=esm --target=node16",
    "prepublishOnly": "nr build"
  },
  "peerDependencies": {
    "eslint": ">=8"
  },
  "dependencies": {
    "@lvjiaxuan/eslint-config-vue": "workspace:^"
  },
  "devDependencies": {
    "eslint": "^8.35.0"
  }
}
